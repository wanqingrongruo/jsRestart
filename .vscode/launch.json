{
    "version": "0.2.0",
    "configurations": [{
            "name": "windowJs", //单独调试js，即可以直接运行js
            "type": "node",
            "request": "launch",
            "program": "${workspaceRoot}/demo04/window.js", //这个配置成你要调试的文件、${file}当前打开的文件
            "stopOnEntry": false,
            "args": [],
            "cwd": "${workspaceRoot}",
            "runtimeExecutable": null,
            "runtimeArgs": [
                "--nolazy"
            ],
            "env": {
                "NODE_ENV": "development"
            },
            "console": "internalConsole",
            "preLaunchTask": "",
            "sourceMaps": false
        },
        {
            "name": "Launch windowHt.html (disable sourcemaps)",
            "type": "chrome",
            "request": "launch",
            "sourceMaps": false,
            "file": "${workspaceRoot}/demo04/window.html" // 每次修改这里的地址
        },
        {
            "name": "jsObject", //单独调试js，即可以直接运行js
            "type": "node",
            "request": "launch",
            "program": "${workspaceRoot}/demo03/jsObject.js", //这个配置成你要调试的文件、${file}当前打开的文件
            "stopOnEntry": false,
            "args": [],
            "cwd": "${workspaceRoot}",
            "runtimeExecutable": null,
            "runtimeArgs": [
                "--nolazy"
            ],
            "env": {
                "NODE_ENV": "development"
            },
            "console": "internalConsole",
            "preLaunchTask": "",
            "sourceMaps": false
        },
        {
            "name": "Launch jsObject.html (disable sourcemaps)",
            "type": "chrome",
            "request": "launch",
            "sourceMaps": false,
            "file": "${workspaceRoot}/demo03/jsObject.html" // 每次修改这里的地址
        },
        {
            "name": "Launch index.html (disable sourcemaps)",
            "type": "chrome",
            "request": "launch",
            "sourceMaps": false,
            "file": "${workspaceRoot}/demo01/base0907.html" // 每次修改这里的地址
        },
        {
            "name": "Launch index02.html (disable sourcemaps)", // 添加新的配置也可以, 这一修改名字方便辨别
            "type": "chrome",
            "request": "launch",
            "sourceMaps": false,
            "file": "${workspaceRoot}/demo02/basic0908.html" // 每次修改这里的地址
        },
        {
            "name": "dom.html (disable sourcemaps)", // 添加新的配置也可以, 这一修改名字方便辨别
            "type": "chrome",
            "request": "launch",
            "sourceMaps": false,
            "file": "${workspaceRoot}/demo02/dom.html" // 每次修改这里的地址
        },
        {
            "type": "chrome",
            "request": "launch",
            "name": "Launch Chrome against localhost",
            "url": "http://localhost:8080",
            "webRoot": "${workspaceRoot}"
        },
        {
            "type": "chrome",
            "request": "attach",
            "name": "Attach to Chrome",
            "port": 9222,
            "webRoot": "${workspaceRoot}"
        }
    ]
}